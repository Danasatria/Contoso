                <div class="section-body">
                    <div class="row">
                        <div class="col">
                            <div class="card">
                                <div class="card-body">
                                    <%
                                        Dim Data
                                        Set Conn = Server.CreateObject("ADODB.Connection")
                                        Set Rec = Server.CreateObject("ADODB.recordset")
                                        Conn.open("Provider=SQLOLEDB; Data Source=MSI; initial catalog=ContosoUniv; User ID=sa;password=dana10")

                                        set Data = Conn.execute("Select * from Course")
                                    %>
                                    <table class="table">
                                        <thead>
                                            <th scope="col">#</th>
                                            <th scope="col">Title</th>
                                            <th scope="col">Credits</th>
                                            <th scope="col">Action</th>
                                        </thead>
                                            <form method="post" action="DeleteC.asp?id=<%=Data("ID")%>" id="del">
                                                
                                            </form>
                                        <tbody>
                                            <%
                                                do while not Data.EOF
                                            %>
                                            <tr>
                                                <td><%=Data("ID")%></td>
                                                <td><%=Data("Title")%></td>
                                                <td><%=Data("Credits")%></td>
                                                <td>
                                                    <a href="Course-form.asp?id=<%=Data("ID")%>" class="btn btn-icon btn-primary btn-sm"><i class="far fa-edit"></i></a>
                                                    <button type="submit" form="del" class="btn btn-icon btn-danger btn-delete btn-sm"><i class="fas fa-trash"></i></button>
                                                </td>
                                            </tr>
                                            <%
                                                Data.MoveNext()
                                                Loop
                                            %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
